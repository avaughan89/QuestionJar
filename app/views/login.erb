<div class='container'>
  <div class='row'>
    <div class='eight columns'><p class='spacer'>Hover Maybe?</p></div>
    <div class='four columns' id='maybeforms'>

      <form action='/users' method='post' id='signup'>
        <p class='form-headers'>Need an Account?</p>
        <input type='text' name='user[username]' placeholder='USERNAME'>
        <input type='email' name='user[email]' placeholder='EMAIL'>
        <input type='password' name='user[password]' placeholder='PASSWORD'>
        <input type='submit' name='SIGN UP' value="Register">
      </form>
    </div>
  </div>
<div class='row'>
    <div class='twelve columns'>
      <img class='checker' src="http://media.giphy.com/media/E87jjnSCANThe/giphy.gif" />
